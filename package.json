{
  "name": "Gudao",
  "description": "Pluto with electron",
  "main": "./main.js",
  "version": "1.3.1",
  "repository": "https://xpluto.cyou",
  "author": "Pluto",
  "license": "CC0-1.0",
  "scripts": {
    "dev": "node ./icons/img",
    "build:mac:arm64": "electron-builder --mac --arm64",
    "build:mac:x64": "electron-builder --mac --x64",
    "build:win": "electron-builder --win"
  },
  "build": {
    "productName": "爱快VPN",
    "appId": "com.ikuaiVPN.pro",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "perMachine": true
    },
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./icons/icon.png",
      "artifactName": "${productName}_setup_${version}.${ext}",
      "target": "dmg",
      "mergeASARs": true,
      "extraResources": [
        {
          "from": "./clash/${arch}/clash-darwin",
          "to": "clash/${arch}/clash-darwin"
        },
        {
          "from": "./clash/${arch}/sysproxy",
          "to": "clash/${arch}/sysproxy"
        },
        {
          "from": "./clash/Country.mmdb",
          "to": "clash/Country.mmdb"
        },
        {
          "from": "./clash/cache.db",
          "to": "clash/cache.db"
        },
        {
          "from": "./clash/config.default.yaml",
          "to": "clash/config.yaml"
        },
        {
          "from": "./clash/test",
          "to": "clash/test"
        }
      ]
    },
    "win": {
      "icon": "./icons/icon.png",
      "artifactName": "${productName}_setup_${version}.${ext}",
      "extraResources": [
        {
          "from": "./clash/win/",
          "to": "clash/"
        }
      ]
    }
  },
  "dependencies": {
    "agent-base": "^7.1.1",
    "ajv": "8.17.1",
    "ajv-formats": "2.1.1",
    "asn1": "0.2.6",
    "assert-plus": "1.0.0",
    "asynckit": "0.4.0",
    "atomically": "1.7.0",
    "aws-sign2": "0.7.0",
    "aws4": "1.13.2",
    "axios": "^1.7.7",
    "bcrypt-pbkdf": "1.0.2",
    "caseless": "0.12.0",
    "combined-stream": "1.0.8",
    "conf": "10.2.0",
    "core-util-is": "1.0.2",
    "dashdash": "1.14.1",
    "debounce-fn": "4.0.0",
    "debug": "4.3.6",
    "delayed-stream": "1.0.0",
    "dot-prop": "6.0.1",
    "ecc-jsbn": "0.1.2",
    "electron-store": "^8.2.0",
    "env-paths": "2.2.1",
    "extend": "3.0.2",
    "extsprintf": "1.3.0",
    "fast-deep-equal": "3.1.3",
    "fast-json-stable-stringify": "2.1.0",
    "fast-uri": "3.0.1",
    "find-up": "3.0.0",
    "follow-redirects": "1.15.8",
    "forever-agent": "0.6.1",
    "form-data": "4.0.0",
    "getpass": "0.1.7",
    "har-schema": "2.0.0",
    "har-validator": "5.1.5",
    "http-signature": "1.2.0",
    "ip-address": "9.0.5",
    "is-obj": "2.0.0",
    "is-typedarray": "1.0.0",
    "isstream": "0.1.2",
    "jsbn": "1.1.0",
    "json-schema": "0.4.0",
    "json-schema-traverse": "0.4.1",
    "json-schema-typed": "7.0.3",
    "json-stringify-safe": "5.0.1",
    "jsprim": "1.4.2",
    "locate-path": "3.0.0",
    "mime-db": "1.52.0",
    "mime-types": "2.1.35",
    "mimic-fn": "3.1.0",
    "ms": "2.1.2",
