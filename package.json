    "whatwg-url": "5.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "electron": "^18.3.5",
    "electron-builder": "^23.3.3",
    "electron-builder-notarize": "^1.5.0"
  }
}
